  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "mongoose": "^5.10.7",
    "mongoose-old-values": "^1.0.0",
    "mongoose-soft-deleting": "^1.0.0"
  },
  "devDependencies": {},
  "scripts": {
    "test": "mocha ./tests/base.js --exit; mocha ./tests/soft-delete.js --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuantumGlitch/mongoose-sub-references-integrity-checker.git"
  },
  "keywords": [
    "mongoose",
    "sub",
    "document",
    "subdocument",
    "reference",
    "references",
    "integrity",
    "consistent",
    "consistency",
